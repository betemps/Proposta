issn = {15399087},
journal = {ACM Transactions on Embedded Computing Systems},
keywords = {Embedded system design,MARTE standard profile,model driven engineering,parallel programming,repetitive structure modeling,system-on-chip},
mendeley-groups = {SystematicLiteratureReview,Teste},
number = {4},
pages = {1--36},
publisher = {ACM},
title = {{A Model-Driven Design Framework for Massively Parallel Embedded Systems}},
